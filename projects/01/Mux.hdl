/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=sel, b=sel, out=selnot);
    Nand(a=a, b=selnot, out=c);
    Nand(a=sel, b=b, out=d);
    Nand(a=c, b=d, out=out);
}
